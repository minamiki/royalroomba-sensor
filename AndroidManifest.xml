<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.royalroomba.sensor" android:versionName="0.5.0" android:versionCode="10">
    <application android:icon="@drawable/icon" android:label="Sensor Platform">
        <activity android:name=".SensorMain"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
	   <service android:enabled="true" android:name=".TorchService" />
    </application>
<uses-permission android:name="android.permission.INTERNET" />
<uses-sdk android:targetSdkVersion="7" android:minSdkVersion="7"></uses-sdk>
</manifest>