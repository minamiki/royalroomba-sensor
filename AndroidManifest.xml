<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      android:versionName="0.1.2" android:versionCode="2" package="com.rr_sensor">
    <application android:icon="@drawable/icon" android:label="Sensor Platform">
        <activity android:name=".SensorMain"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:enabled="true" android:name="SensorService"/>
    </application>
<uses-permission android:name="android.permission.INTERNET"></uses-permission>





</manifest>